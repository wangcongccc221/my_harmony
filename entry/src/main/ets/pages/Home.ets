  private async handleEndProcessing(clearDeviceData: boolean): Promise<void> {
    const dbSync = getDatabaseSync()
    const gdi = getGlobalDataInterface()
    const batchId = dbSync.getCurrentBatchId()
    const global = gdi.getGlobalData()
    if (batchId === 0 || global.startTime === 0 || global.totalYield === 0) {
      this.omniUIUtils.showError(t('å½“å‰çŠ¶æ€æ— æ³•ç»“æŸåŠ å·¥ï¼', 'å½“å‰çŠ¶æ€æ— æ³•ç»“æŸåŠ å·¥ï¼'))
      return
    }

    try {
      await dbSync.saveNow()
      const sender = getConfigSender()
      const deviceOk = clearDeviceData ? await sender.sendClearData() : await sender.sendSaveData()
      if (!deviceOk) {
        this.omniUIUtils.showError(t('ç»“æŸåŠ å·¥æŒ‡ä»¤å‘é€å¤±è´¥ï¼', 'ç»“æŸåŠ å·¥æŒ‡ä»¤å‘é€å¤±è´¥ï¼'))
        return
      }

      const endOk = await dbSync.endCurrentBatch()
      if (!endOk) {
        this.omniUIUtils.showError(t('ç»“æŸåŠ å·¥å¤±è´¥ï¼šæ‰¹æ¬¡æœªä¿å­˜', 'ç»“æŸåŠ å·¥å¤±è´¥ï¼šæ‰¹æ¬¡æœªä¿å­˜'))
        return
      }

      gdi.stopProcessing()
      gdi.clearData()

      AlertDialog.show({
        title: t('æç¤º', 'æç¤º'),
        message: t('æœ¬æ¬¡åŠ å·¥ç»“æŸï¼æ˜¯å¦æ‰“å°æŠ¥è¡¨ï¼Ÿ', 'æœ¬æ¬¡åŠ å·¥ç»“æŸï¼æ˜¯å¦æ‰“å°æŠ¥è¡¨ï¼Ÿ'),
        primaryButton: {
          value: t('å¦', 'å¦'),
          action: () => {}
        },
        secondaryButton: {
          value: t('æ˜¯', 'æ˜¯'),
          action: () => {
