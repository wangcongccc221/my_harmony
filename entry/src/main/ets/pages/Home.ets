  private async confirmSaveLanguage(): Promise<void> {
    const langName = this.languageNameInput.trim()
    if (!langName || langName.length === 0) {
      this.omniUIUtils.showInfo(t('è¯·è¾“å…¥è¯­è¨€åç§°', 'è¯·è¾“å…¥è¯­è¨€åç§°'))
